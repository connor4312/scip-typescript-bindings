{
  "name": "@sourcegraph/scip",
  "version": "0.1.0",
  "description": "Generated TypeScript definitions for the Semantic Code Intelligence Protocol.",
  "repository": "https://github.com/sourcegraph/scip/bindings/typescript",
  "author": "Sourcegraph",
  "license": "Apache-2.0",
  "sideEffects": false,
  "private": false,
  "scripts": {
    "compile:fetch": "curl https://raw.githubusercontent.com/sourcegraph/scip/main/scip.proto > scip.proto",
    "compile:js": "pbjs -t static-module -o scip.js -w commonjs scip.proto",
    "compile:ts": "pbts -o scip.d.ts scip.js",
    "compile": "npm run compile:fetch && npm run compile:js && npm run compile:ts"
  },
  "main": "./scip",
  "dependencies": {
    "protobufjs": "^7.3.2"
  },
  "devDependencies": {
    "protobufjs-cli": "^1.1.2"
  }
}
